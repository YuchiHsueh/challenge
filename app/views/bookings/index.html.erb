<h2>Booking Index</h2>
<ul>
  <% @bookings.each do |booking| %>
    <li> Booking ID <%= booking.id %> Start date: <%= booking.start_date.to_date %> End date: <%= booking.end_date.to_date %></li>
    <%= link_to "Delete booking", booking_path(booking), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}, class:"btn btn-outline-dark"%>
  <% end %>
</ul>
